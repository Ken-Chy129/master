<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.ken.master.server.mapper.RecordMapper">


    <select id="selectByFieldId" resultType="cn.ken.master.server.model.entity.RecordDO">
        select * from master_record
        where field_id = #{fieldId}
    </select>
</mapper>
